      RowBox[{
       SubscriptBox["y", "1"], "(", "x", ")"}], " ", "*", " ", 
      RowBox[{"u", "(", "x", ")"}]}], " ", "+", " ", 
     RowBox[{
      RowBox[{
       SubscriptBox["y", "2"], "(", "x", ")"}], " ", "*", " ", 
      RowBox[{"v", "(", "x", ")"}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"where", " ", 
    RowBox[{
     SubscriptBox["y", "1"], "(", "x", ")"}], "  ", "and", "  ", 
    RowBox[{
     SubscriptBox["y", "2"], "(", "x", ")"}], " ", "are", " ", "two", " ", 
    "linearly", " ", "independent", " ", "solutions", " ", "of", " ", "above",
     " ", "homogeneous", " ", "equation"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"and", " ", 
      RowBox[{"u", "(", "x", ")"}]}], " ", "=", "  ", 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"-", 
        FractionBox[
         RowBox[{
          RowBox[{
           SubscriptBox["y", "2"], "(", "x", ")"}], " ", "*", " ", 
          RowBox[{"r", "(", "x", ")"}], " "}], 
         RowBox[{
          RowBox[{"a", "[", "x", "]"}], " ", "*", " ", 
          RowBox[{"W", "(", "x", ")"}]}]]}], "dx"}]}]}], ",", " ", 
