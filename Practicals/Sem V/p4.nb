Cell[TextData[{
 StyleBox["2. To get particular integral of the non-homogeneous differential \
equation which is given by \n",
  FontSize->16],
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["y", "p"], "(", "x", ")"}], " ", "=", " ", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["y", "1"], "(", "x", ")"}], " ", "*", " ", 
      RowBox[{"u", "(", "x", ")"}]}], " ", "+", " ", 
     RowBox[{
      RowBox[{
       SubscriptBox["y", "2"], "(", "x", ")"}], " ", "*", " ", 
      RowBox[{"v", "(", "x", ")"}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"where", " ", 
    RowBox[{
     SubscriptBox["y", "1"], "(", "x", ")"}], "  ", "and", "  ", 
    RowBox[{
     SubscriptBox["y", "2"], "(", "x", ")"}], " ", "are", " ", "two", " ", 
    "linearly", " ", "independent", " ", "solutions", " ", "of", " ", "above",
     " ", "homogeneous", " ", "equation"}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"and", " ", 
      RowBox[{"u", "(", "x", ")"}]}], " ", "=", "  ", 
     RowBox[{"\[Integral]", 
      RowBox[{
       RowBox[{"-", 
        FractionBox[
         RowBox[{
          RowBox[{
           SubscriptBox["y", "2"], "(", "x", ")"}], " ", "*", " ", 
          RowBox[{"r", "(", "x", ")"}], " "}], 
         RowBox[{
          RowBox[{"a", "[", "x", "]"}], " ", "*", " ", 
          RowBox[{"W", "(", "x", ")"}]}]]}], "dx"}]}]}], ",", " ", 
    RowBox[{
     RowBox[{"v", "(", "x", ")"}], " ", "=", " ", 
     RowBox[{"\[Integral]", 
      RowBox[{
       FractionBox[
        RowBox[{
         RowBox[{
          SubscriptBox["y", "1"], "(", "x", ")"}], " ", "*", " ", 
         RowBox[{"r", "(", "x", ")"}], " "}], 
        RowBox[{
         RowBox[{"a", "[", "x", "]"}], " ", "*", " ", 
         RowBox[{"W", "(", "x", ")"}]}]], "dx"}]}]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{"where", " ", ",", "\[IndentingNewLine]", "\t", 
    RowBox[{
     RowBox[{"W", "(", "x", ")"}], "  ", "is", "  ", "Wronskian", " ", "of", 
     RowBox[{
      SubscriptBox["y", "1"], "(", "x", ")"}], " ", "and", " ", 
     RowBox[{
      SubscriptBox["y", "2"], "(", "x", ")"}]}], ",", "\[IndentingNewLine]", 
    "\t", 
    RowBox[{
     RowBox[{"a", "(", "x", ")"}], " ", "is", " ", "the", " ", "coefficient", 
     " ", "of", " ", 
     FractionBox[
      RowBox[{
       SuperscriptBox["d", "2"], "y"}], 
      RowBox[{"d", " ", 
       SuperscriptBox["x", "2"]}]]}]}], 
