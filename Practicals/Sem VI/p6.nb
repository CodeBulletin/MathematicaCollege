Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"DividedDiff", "[", 
    RowBox[{"x_", ",", " ", "y_"}], "]"}], " ", ":=", " ", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Return", "[", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"y", "[", 
          RowBox[{"[", "i", "]"}], "]"}], " ", "/", " ", 
         RowBox[{"Product", "[", 
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"i", "==", "j"}], ",", " ", "1", ",", " ", 
             RowBox[{
              RowBox[{"x", "[", 
               RowBox[{"[", "i", "]"}], "]"}], " ", "-", " ", 
              RowBox[{"x", "[", 
               RowBox[{"[", "j", "]"}], "]"}]}]}], "]"}], ",", " ", 
           RowBox[{"{", 
            RowBox[{"j", ",", " ", "1", ",", " ", 
             RowBox[{"Length", "[", "x", "]"}]}], "}"}]}], "]"}]}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"i", ",", " ", "1", ",", " ", 
          RowBox[{"Length", "[", "y", "]"}]}], "}"}]}], "]"}], "]"}], ";"}], 
    "\[IndentingNewLine]", ")"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"NewtonsPolynomial", "[", 
    RowBox[{"x_", ",", " ", "y_", ",", " ", "var_"}], "]"}], ":=", " ", 
   RowBox[{"(", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Return", "[", 
      RowBox[{"Simplify", "[", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"DividedDiff", "[", 
           RowBox[{
            RowBox[{"x", "[", 
             RowBox[{"[", 
              RowBox[{"1", ";;", " ", "i"}], "]"}], "]"}], ",", " ", 
            RowBox[{"y", "[", 
             RowBox[{"[", 
              RowBox[{"1", ";;", " ", "i"}], "]"}], "]"}]}], "]"}], "*", 
          RowBox[{"Product", "[", 
           RowBox[{
            RowBox[{"var", " ", "-", " ", 
             RowBox[{"x", "[", 
              RowBox[{"[", "j", "]"}], "]"}]}], ",", " ", 
            RowBox[{"{", 
             RowBox[{"j", ",", " ", "1", ",", " ", 
              RowBox[{"i", "-", "1"}]}], "}"}]}], "]"}]}], ",", " ", 
         RowBox[{"{", 
          RowBox[{"i", ",", " ", "1", ",", " ", 
           RowBox[{"Length", "[", "y", "]"}]}], "}"}]}], "]"}], "]"}], "]"}], 
     ";"}], "\[IndentingNewLine]", ")"}]}], ";"}]}], "Input",
