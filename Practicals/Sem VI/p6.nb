                 RowBox[{"[", "j", "]"}], "]"}]}], ")"}]}]}], "]"}], ",", " ", 
           RowBox[{"{", 
            RowBox[{"j", ",", " ", "1", " ", ",", " ", "n"}], "}"}]}], 
